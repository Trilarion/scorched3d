  <option>
    <name>ColorComponentSize</name>
    <value>0</value>
    <defaultvalue>0</defaultvalue>
  </option>
  <!-- The number of bits per pixel to use for the display (0 = current display bbp) -->
  <option>
    <name>BitsPerPixel</name>
    <value>0</value>
    <defaultvalue>0</defaultvalue>
  </option>
  <!-- The number of bits requested for the depth buffer -->
  <option>
    <name>DepthBufferBits</name>
    <value>24</value>
    <defaultvalue>24</defaultvalue>
  </option>
  <!-- Use the detail texture on the landscape -->
  <option>
    <name>DetailTexture</name>
    <value>on</value>
    <defaultvalue>on</defaultvalue>
  </option>
  <!-- Show the tank angles and amounts in hex -->
  <option>
    <name>UseHexidecimal</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Use double buffering -->
  <option>
    <name>DoubleBuffer</name>
    <value>on</value>
    <defaultvalue>on</defaultvalue>
  </option>
  <!-- Whether to use full screen mode or not -->
  <option>
    <name>FullScreen</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Completely clear the screen on each frame -->
  <option>
    <name>FullClear</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Do not use texture area replacing -->
  <option>
    <name>NoGLTexSubImage</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Do not use any OpenGL extensions -->
  <option>
    <name>NoGLExt</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Do not use more than one texture per face -->
  <option>
    <name>NoGLMultiTex</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Do not compile vertex arrays -->
  <option>
    <name>NoGLCompiledArrays</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Do not use combined textures -->
  <option>
    <name>NoGLEnvCombine</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Do not use cube maps -->
  <option>
    <name>NoGLCubeMap</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Do not use hardware mipmaps -->
  <option>
    <name>NoGLHardwareMipmaps</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Invert the up and down cursor key directions -->
  <option>
    <name>InvertUpDownKeys</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Do not use the sound. -->
  <option>
    <name>NoSound</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Show landscape normals on the landscape -->
  <option>
    <name>DrawNormals</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Do not fill in the landscape -->
  <option>
    <name>DrawLines</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Do not show the full lense flare effect -->
  <option>
    <name>NoLenseFlare</name>
    <value>on</value>
    <defaultvalue>on</defaultvalue>
  </option>
  <!-- Do not texture tanks -->
  <option>
    <name>NoTankSkins</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Do not draw water -->
  <option>
    <name>DrawWater</name>
    <value>on</value>
    <defaultvalue>on</defaultvalue>
  </option>
  <!-- Draw the names above the tanks -->
  <option>
    <name>DrawPlayerNames</name>
    <value>on</value>
    <defaultvalue>on</defaultvalue>
  </option>
  <!-- Draw the aiming sight infront of the tanks -->
  <option>
    <name>DrawPlayerSight</name>
    <value>on</value>
    <defaultvalue>on</defaultvalue>
  </option>
  <!-- Draw the player color triangle over the tank -->
  <option>
    <name>DrawPlayerColor</name>
    <value>on</value>
    <defaultvalue>on</defaultvalue>
  </option>
  <!-- Draw the health bars above the tank -->
  <option>
    <name>DrawPlayerHealth</name>
    <value>on</value>
    <defaultvalue>on</defaultvalue>
  </option>
  <!-- Do not use ROAM algorithm -->
  <option>
    <name>NoROAM</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Texture the landscape -->
  <option>
    <name>LandscapeTexture</name>
    <value>on</value>
    <defaultvalue>on</defaultvalue>
  </option>
  <!-- Do not use multiple sky layers (just 1) -->
  <option>
    <name>NoSkyLayers</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Do not animate the sky -->
  <option>
    <name>NoSkyMovement</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Show the current number of frames per second (FPS) -->
  <option>
    <name>FrameTimer</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Do not animate the water -->
  <option>
    <name>NoWaterMovement</name>
    <value>off</value>
    <defaultvalue>off</defaultvalue>
  </option>
  <!-- Show help bubbles for items that have it -->
  <option>
    <name>ShowContextHelp</name>
    <value>on</value>
    <defaultvalue>on</defaultvalue>
  </option>
  <!-- The texture details setting -->
  <option>
    <name>TexureSize</name>
    <value>1</value>
    <defaultvalue>1</defaultvalue>
  </option>
  <!-- The tank details setting -->
  <option>
    <name>TankDetail</name>
    <value>2</value>
    <defaultvalue>2</defaultvalue>
  </option>
  <!-- The number of allowed on screen effects -->
  <option>
    <name>EffectsDetail</name>
    <value>1</value>
    <defaultvalue>1</defaultvalue>
  </option>
  <!-- The window width to use (in pixels) -->
  <option>
    <name>ScreenWidth</name>
    <value>640</value>
    <defaultvalue>640</defaultvalue>
  </option>
  <!-- The window height to use (in pixels) -->
  <option>
    <name>ScreenHeight</name>
    <value>480</value>
    <defaultvalue>480</defaultvalue>
  </option>
  <!-- The unique string given to any servers to uniquely identify the client. -->
  <option>
    <name>UniqueUserId</name>
    <value>6930-21571-12628</value>
    <defaultvalue></defaultvalue>
  </option>
</options>
